class while_test{
	
	
	public boolean method()
	{
		int ${condition}=1;
		int ${validator}=0;
		while(${condition}!=0)
		{
			${validator}++;
			if(${validator}==10)
			{
				${condition} = 0;
			}
		}
		${validator}=0;
		do{
			${validator}++;
			if(${validator}==10)
			{
				${condition} = 1;
			}
		}while(${condition}!=1)
	}
}