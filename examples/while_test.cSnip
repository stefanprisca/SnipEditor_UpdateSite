class while_test{
	
	
	public boolean method()
	{
		int ${probe}=true;
		int ${validator}=0;
	
		while(${probe}!=false)
		{
			${validator}++;
			if(${validator}==10)
			{
				${probe} = false;
			}
		}
		${validator}=0;
		do{
			${validator}++;
			if(${validator}==10)
			{
				${probe} = true;
			}
		}while(${probe}!=true)
	}
}
